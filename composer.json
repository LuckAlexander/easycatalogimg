{
    "name": "tm/easycatalogimg",
    "type": "magento-module",
    "homepage": "http://gitlab.templates-master.com/tm/easycatalogimg",
    "license": "OSL-3.0",
    "description": "Templates Master easy catalog images module",
    "version": "2.0.2",
    "authors": [
        {
            "name": "Template Masters Team",
            "email": "support@templates-master.com"
        }
    ],
    "require": {
        "magento-hackathon/magento-composer-installer": "*",
        "tm/core": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.templates-master.com"
        }
    ],
    "extra": {
        "map": [
            ["app/code/community/TM/EasyCatalogImg", "app/code/community/TM/EasyCatalogImg"],
